{
  "name": "glitchbread-redirector",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "wrangler dev",
    "deploy": "wrangler publish",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}'",
    "lint:eslint": "eslint --max-warnings=0 src",
